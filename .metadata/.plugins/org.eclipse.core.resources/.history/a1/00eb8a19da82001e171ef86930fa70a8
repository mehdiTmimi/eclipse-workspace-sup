package p1;

public class Human {
	private String nomComplet;
	private int age;

	
	// getters
	public String getNomComplet() {
		return this.nomComplet;
	}
	public int getAge() {
		return this.age;
	}
	//setters
	public void setNomComplet(String nomComplet) {
		this.nomComplet=nomComplet;
	}
	public void setAge(int age)
	{
		if(age>=1 && age<=120)
			this.age=age;
	}

}
